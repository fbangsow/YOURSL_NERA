<apex:page standardController="OrganizationAccountMember__c">
    <apex:form >
        <apex:pageBlock title="My Content" mode="edit">
            <apex:pageBlockButtons location="bottom">
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="My Content Section" columns="2">
            
                <apex:inputField value="{!OrganizationAccountMember__c.Account__c}"/>
                <apex:inputField value="{!OrganizationAccountMember__c.Organization__c}"/>
                
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>